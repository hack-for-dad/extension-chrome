{
  "manifest_version": 3,
  "name": "ส่วนเสริมสำหรับพ่อหลวง",
  "version": "0.0.0.1",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "css": ["styles.css"]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["<all_urls>"]
    }
  ]
}
